<%= form_for(@city) do |f| %>
<%= render 'layouts/error_messages', :object => @city %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :state %>
<%= f.text_field :state %>
<%= f.label :official_status %>
<%= f.select :official_status, City.official_statuses.keys %>
<%= f.submit %>
<% end %>
