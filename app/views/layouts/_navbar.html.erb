Welcome <%= current_user.username %><br/>
<br>
<%= link_to "About Sanctuary Cities", about_path %><br/>
<%= link_to "All Cities", cities_path %><br/>
<% if current_user.admin? %>
<%= link_to "All Users", users_path %><br/>
<% end %>

<% if !current_user.id.nil? %>
<%= link_to "Profile", user_path(current_user) %><br/>
<% end %>

<%= link_to "Back", :back %><br/>

<% if current_user.username == "guest" %>
<%= button_to "Login", new_user_session_path, method: :get %>
<%= button_to "Login with Google", user_google_oauth2_omniauth_authorize_path %>
<%= button_to "Sign up", new_user_registration_path, method: :get %><br/>
<% else %>
<%= button_to "Sign out", destroy_user_session_path, method: :delete %><br/>
<% end %>
<hr>
